<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.groupid.Controller.WorkbenchController">
   <top>
      <HBox prefHeight="86.0" prefWidth="720.0" BorderPane.alignment="CENTER">
         <children>
            <Button onAction="#toSendClick" text="Новое письмо" />
         </children>
      </HBox>
   </top>
   <center>
      <ScrollPane prefHeight="480.0" prefWidth="720.0" BorderPane.alignment="CENTER">
        <content>
          <AnchorPane prefHeight="480.0" prefWidth="720.0">
               <children>
                  <VBox fx:id="VB" prefHeight="480.0" prefWidth="720.0">
                     <children>
                        <Label text="Список писем:" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </center>
</BorderPane>
